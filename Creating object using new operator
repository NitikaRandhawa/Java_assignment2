class Student {
   int rno;
   String name;
   public Student(int r, String n) {
      rno = r;
      name = n;
   }
   void display() {
      System.out.println("Roll Number: " + rno);
      System.out.println("Name: " + name);
   }
}
public class Demo {
   public static void main(String[] args) {
      Student s = new Student(15, "Peter Smith");
      s.display();
   }
}
